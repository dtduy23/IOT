    {
        "id": "73bffa04a6939828",
        "type": "ui_template",
        "z": "54a42a8de7bec392",
        "group": "ba6741385424d3f0",
        "name": "Chat Panel",
        "order": 1,
        "width": 0,
        "height": 0,
        "format": "<style>\n  .chat-wrapper {\n    border-radius: 12px;\n    background: #ffffff;\n    padding: 10px 16px 16px;\n    height: 400px;\n    display: flex;\n    flex-direction: column;\n    box-shadow: 0 2px 6px rgba(0,0,0,0.05);\n  }\n\n  .chat-title {\n    font-size: 20px;\n    font-weight: 600;\n    margin-bottom: 8px;\n    color: #1abc9c;\n  }\n\n  .chat-box {\n    flex: 1;\n    overflow-y: auto;\n    padding-right: 4px;\n  }\n\n  .chat-message {\n    max-width: 80%;\n    margin: 4px 0;\n    padding: 8px 12px;\n    border-radius: 16px;\n    font-size: 14px;\n    line-height: 1.4;\n    white-space: pre-wrap;\n  }\n\n  .chat-message.user {\n    margin-left: auto;\n    background: #e8f7ff;\n  }\n\n  .chat-message.assistant {\n    margin-right: auto;\n    background: #f0f0f0;\n  }\n\n  .chat-sender {\n    font-size: 11px;\n    opacity: 0.7;\n    margin-bottom: 2px;\n  }\n</style>\n\n<div class=\"chat-wrapper\">\n  <div class=\"chat-title\">Chat</div>\n  <div id=\"chat-box\" class=\"chat-box\">\n    <div ng-repeat=\"m in chat track by $index\"\n         class=\"chat-message\"\n         ng-class=\"{'user': m.role === 'user', 'assistant': m.role === 'assistant'}\">\n      <div class=\"chat-sender\"\n           ng-bind=\"m.role === 'user' ? 'Bạn' : 'Assistant'\"></div>\n      <div ng-bind=\"m.text\"></div>\n    </div>\n  </div>\n</div>\n\n<script>\n  (function(scope) {\n    // Mảng lưu hội thoại trong UI (chỉ ở phía trình duyệt)\n    scope.chat = scope.chat || [];\n\n    scope.$watch('msg', function(msg) {\n      if (!msg) return;\n      if (typeof msg.text === 'undefined' ||\n          typeof msg.role === 'undefined') return;\n\n      scope.chat.push({\n        role: msg.role,\n        text: msg.text\n      });\n\n      // Tự cuộn xuống cuối\n      setTimeout(function() {\n        var box = document.getElementById('chat-box');\n        if (box) {\n          box.scrollTop = box.scrollHeight;\n        }\n      }, 0);\n    });\n  })(scope);\n</script>\n",
        "storeOutMessages": true,
        "fwdInMessages": true,
