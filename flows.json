        "id": "323a89e249db492e",
        "type": "function",
        "z": "54a42a8de7bec392",
        "name": "BuildGeminiRequest",
        "func": "// msg.fullPrompt đã được tạo ở node Prepare Chat\n\nmsg.payload = {\n  contents: [\n    {\n      role: \"user\",\n      parts: [\n        { text: msg.fullPrompt }\n      ]\n    }\n  ]\n};\n\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
